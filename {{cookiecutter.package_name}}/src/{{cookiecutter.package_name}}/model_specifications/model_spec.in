components:
    vivarium_public_health:
        population:
            - BasePopulation()
            - Mortality()
        metrics:
            - DisabilityObserver()
            - MortalityObserver()

    # {{cookiecutter.package_name}}.components:

configuration:
    input_data:
        location: {% raw %}{{ location_proper }}{% endraw %}
        input_draw_number: 0
        artifact_path: /share/costeffectiveness/artifacts/{{cookiecutter.package_name}}/{% raw %}{{ location_sanitized }}{% endraw %}.hdf
    interpolation:
        order: 0
        extrapolate: False
    randomness:
        map_size: 1_000_000
        key_columns: ['entrance_time', 'age']
        random_seed: 0
    time:
        start:
            year: 2005
            month: 7
            day: 2
        end:
            year: 2010
            month: 7
            day: 2
        step_size: 1 # Days
    population:
        population_size: 100
        age_start: 0
        age_end: 100

    metrics:
        disability:
            by_age: False
            by_sex: False
            by_year: False
        mortality:
            by_age: False
            by_sex: False
            by_year: False
